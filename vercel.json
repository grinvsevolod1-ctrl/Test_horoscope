{
  "crons": [
    {
      "path": "/api/cron/daily",
      "schedule": "0 5 * * *"
    }
  ],
  "trailingSlash": false,
  "headers": [
    {
      "source": "/api/webhooks/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
